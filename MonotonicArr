bool isMonotonic(int* nums, int numsSize){
int i,flag1=0,flag2=0,flag3=0;
for(i=0;i<numsSize-1;i++){
    if(nums[i]>nums[i+1]){
       flag1++;
    }
      if(nums[i]<nums[i+1]){
       flag2++;
    }
      if(nums[i]!=nums[i+1]){
       flag3++;
    }
}
    if(flag1!=0 && flag2!=0 && flag3!=0)
        return false;
    return true;
}
